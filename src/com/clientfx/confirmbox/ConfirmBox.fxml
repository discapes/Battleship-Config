<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="111.0" prefWidth="357.0" styleClass="background" stylesheets="/com/clientfx/css/darktheme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clientfx.confirmbox.ConfirmBoxController">
	<children>
		<HBox alignment="CENTER" layoutY="50.0" prefHeight="61.0" prefWidth="359.0" spacing="10.0">
			<children>
				<Button mnemonicParsing="false" onAction="#confirmYesClicked" styleClass="background" text="Yes" />
				<Button mnemonicParsing="false" onAction="#confirmNoClicked" text="No" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</HBox>
		<HBox alignment="CENTER" prefHeight="50.0" prefWidth="359.0">
			<children>
				<Label fx:id="confirmLabel" contentDisplay="CENTER" styleClass="eighteen-text" text="Label">
               <font>
                  <Font size="7.0" />
               </font></Label>
			</children>
		</HBox>
	</children>
</AnchorPane>
